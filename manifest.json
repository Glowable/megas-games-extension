{
  "manifest_version": 2,
  "name": "Join Megas Games",
  "version": "1.1",
  "description": "This extension makes you join Megas Games",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Click to redirect",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["popup.js"]
  }
}
